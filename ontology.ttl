@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .  
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .  
@prefix owl: <http://www.w3.org/2002/07/owl#> .  
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .  
@prefix soft: <http://example.org/software#> .  
@prefix cpe: <http://example.org/cpe#> .  
@prefix cve: <http://example.org/cve#> .  
  
##############################  
# Classes  
##############################  
soft:Software rdf:type owl:Class .  
soft:WebServer rdf:type owl:Class ;  
    rdfs:subClassOf soft:Software .  
soft:Database rdf:type owl:Class ;  
    rdfs:subClassOf soft:Software .  
  
cpe:Platform rdf:type owl:Class ;  
    rdfs:subClassOf soft:Software .  
  
cve:Vulnerability rdf:type owl:Class .  
  
##############################  
# Object properties  
##############################  
cve:affects rdf:type owl:ObjectProperty ;  
    owl:inverseOf soft:isAffectedBy ;  
    rdfs:domain cve:Vulnerability ;  
    rdfs:range soft:Software .  
  
soft:dependsOn rdf:type owl:ObjectProperty ;  
    rdfs:domain soft:Software ;  
    rdfs:range soft:Software ;  
    rdf:type owl:TransitiveProperty .  
  
owl:sameAs rdf:type owl:SymmetricProperty, owl:TransitiveProperty .  
  
##############################  
# Data properties  
##############################  
soft:vendor rdf:type owl:DatatypeProperty .  
soft:product rdf:type owl:DatatypeProperty .  
soft:version rdf:type owl:DatatypeProperty .  
  
cve:minVersion rdf:type owl:DatatypeProperty .  
cve:maxVersion rdf:type owl:DatatypeProperty .  
cve:severity rdf:type owl:DatatypeProperty .  
cve:cvssScore rdf:type owl:DatatypeProperty .  